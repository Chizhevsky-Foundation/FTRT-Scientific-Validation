    "                   (row['ftrt_calculated'], row['magnitude']),\n",
    "                   fontsize=8,\n",
    "                   xytext=(5, 5),\n",
    "                   textcoords='offset points')\n",
    "\n",
    "# Calcular correlaci√≥n\n",
    "r, p_val = stats.pearsonr(ftrt_data['ftrt_calculated'], ftrt_data['magnitude'])\n",
    "ax.text(0.05, 0.95, \n",
    "        f'r = {r:.3f}\\np = {p_val:.4f}\\nR¬≤ = {r**2:.3f}',\n",
    "        transform=ax.transAxes,\n",
    "        verticalalignment='top',\n",
    "        bbox=dict(boxstyle='round', facecolor='wheat', alpha=0.8),\n",
    "        fontsize=12)\n",
    "\n",
    "ax.set_xlabel('FTRT', fontsize=14, fontweight='bold')\n",
    "ax.set_ylabel('Magnitud X-Class', fontsize=14, fontweight='bold')\n",
    "ax.set_title('Correlaci√≥n FTRT vs Magnitud de Tormentas Solares', \n",
    "            fontsize=16, fontweight='bold')\n",
    "ax.legend(loc='upper left', fontsize=10)\n",
    "ax.grid(True, alpha=0.3)\n",
    "\n",
    "# Colorbar\n",
    "cbar = plt.colorbar(scatter, ax=ax)\n",
    "cbar.set_label('√çndice Kp', fontsize=12)\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()\n",
    "\n",
    "print(f\"\\n‚úì Correlaci√≥n de Pearson: r = {r:.4f}\")\n",
    "print(f\"‚úì P-value: {p_val:.6f}\")\n",
    "print(f\"‚úì Coeficiente de Determinaci√≥n: R¬≤ = {r**2:.4f}\")"
   ]
