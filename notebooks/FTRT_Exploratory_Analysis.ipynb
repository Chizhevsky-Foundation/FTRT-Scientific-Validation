  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Scatter plot principal\n",
    "fig, ax = plt.subplots(figsize=(12, 8))\n",
    "\n",
    "# Scatter con colores seg√∫n Kp\n",
    "scatter = ax.scatter(ftrt_data['ftrt_calculated'], \n",
    "                    ftrt_data['magnitude'],\n",
    "                    s=200, \n",
    "                    c=ftrt_data['kp'],\n",
    "                    cmap='YlOrRd',\n",
    "                    alpha=0.7,\n",
    "                    edgecolors='black',\n",
    "                    linewidth=2)\n",
    "\n",
    "# L√≠nea de regresi√≥n\n",
    "z = np.polyfit(ftrt_data['ftrt_calculated'], ftrt_data['magnitude'], 1)\n",
    "p = np.poly1d(z)\n",
    "x_line = np.linspace(ftrt_data['ftrt_calculated'].min(), \n",
    "                     ftrt_data['ftrt_calculated'].max(), 100)\n",
    "ax.plot(x_line, p(x_line), \"r--\", linewidth=2, label='Regresi√≥n Lineal')\n",
    "\n",
    "# L√≠neas de umbral\n",
    "ax.axvline(x=2.5, color='orange', linestyle=':', linewidth=2, label='Umbral Cr√≠tico')\n",
    "ax.axvline(x=4.0, color='red', linestyle=':', linewidth=2, label='Umbral Extremo')\n",
    "\n",
    "# Anotaciones para eventos clave\n",
    "for idx, row in ftrt_data.iterrows():\n",
