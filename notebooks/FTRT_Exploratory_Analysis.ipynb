    "axes[1].set_ylabel('Frecuencia', fontsize=12)\n",
    "axes[1].set_title('Distribuci√≥n de Magnitud', fontsize=14, fontweight='bold')\n",
    "axes[1].grid(True, alpha=0.3, axis='y')\n",
    "\n",
    "# Boxplot por nivel de alerta\n",
    "alert_order = ['NORMAL', 'ELEVADO', 'CR√çTICO', 'EXTREMO']\n",
